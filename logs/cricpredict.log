export async function saveUserWallet(telegramId, { withdrawal_address, deposit_address }) {
  await pool.query(
    `INSERT INTO users (telegram_id, withdrawal_address, deposit_address, last_active)
     VALUES ($1, $2, $3, NOW())
     ON CONFLICT (telegram_id)
