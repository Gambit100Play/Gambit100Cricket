// =====================================================
// ğŸ’¾ BULK SAVE MULTIPLE MATCHES (Trust precomputed local times)
// =====================================================
export async function saveMatches(matches = []) {
  if (!matches || matches.length === 0) {
    console.log("âš ï¸ [DB] No matches provided to saveMatches().");
    return 0;
  }

  const client = await pool.connect();
  let saved = 0;

  try {
    await client.query("BEGIN");

    for (const match of matches) {
      try {
        // âœ… Step 1: Use India-local values provided by API layer directly
        let startDate = match.start_date || null;
        let startTimeLocal = match.start_time_local || null;

        // Fallback (rare case): derive if API didnâ€™t send them
        if (!startDate && match.start_time) {
          const dt = DateTime.fromISO(match.start_time, { zone: "UTC" }).setZone("Asia/Kolkata");
          startDate = dt.toFormat("yyyy-LL-dd");
          startTimeLocal = dt.toFormat("HH:mm:ss");
        }

        // âœ… Step 2: Skip very old matches (>6h)
        if (match.start_time) {
          const matchTime = DateTime.fromISO(match.start_time, { zone: "UTC" }).setZone("Asia/Kolkata");
          const now = DateTime.now().setZone("Asia/Kolkata");
          const hoursDiff = now.diff(matchTime, "hours").hours;
          if (hoursDiff > 6 && match.status !== "live") {
            console.log(`â­ï¸ [Skip] Old match (>6h): ${match.name}`);
            continue;
          }
        }

        // âœ… Step 3: Save directly
        await client.query(
          `INSERT INTO matches (
              id, name, start_time, start_date, start_time_local,
              status, score, api_payload, updated_at
           )
           VALUES ($1, $2, $3, $4, $5, $6, $7, $8, NOW())
           ON CONFLICT (id) DO UPDATE
             SET name = EXCLUDED.name,
                 start_time = EXCLUDED.start_time,
                 start_date = EXCLUDED.start_date,
                 start_time_local = EXCLUDED.start_time_local,
                 status = EXCLUDED.status,
                 score = EXCLUDED.score,
                 api_payload = EXCLUDED.api_payload,
                 updated_at = NOW()`,
          [
            match.id,
            match.name,
            match.start_time,          // already UTC ISO from cricbuzzApi.js
            startDate,                 // precomputed India date
            startTimeLocal,            // precomputed India time
            match.status,
            match.score || null,
            match.api_payload ? JSON.stringify(match.api_payload) : null,
          ]
        );

        saved++;
        console.log(`ğŸ’¾ [DB] Saved: ${match.name} (${match.status}) [${startDate} ${startTimeLocal} IST]`);
      } catch (innerErr) {
        console.warn(`âš ï¸ [DB] Skipped ${match.name}: ${innerErr.message}`);
      }
    }

    await client.query("COMMIT");
    console.log(`ğŸ’¾ [DB] Bulk saved ${saved}/${matches.length} matches successfully.`);
    return saved;
  } catch (err) {
    await client.query("ROLLBACK");
    console.error("âŒ [DB] saveMatches() transaction failed:", err.message);
    return 0;
  } finally {
    client.release();
  }
}


